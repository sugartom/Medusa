			<config>
				<input>GENMETADATA</input>
			</config>
			<notification>http://68.181.99.207:9000</notification>
		</stage>

		<connector>
			<src>GenerateMetadata</src> 
			<dst> <success>UploadData</success> <failure>GenerateMetadata</failure> </dst>
		</connector>
